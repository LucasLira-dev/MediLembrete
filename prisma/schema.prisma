generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
  engineType = "library" 
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model dbMedicamento {
  id      Int     @id @default(autoincrement())
  dosagem Int
  horario String
  userId  Int
  usuario usuario @relation(fields: [userId], references: [userId], onDelete: Cascade)

  @@index([userId], map: "dbMedicamento_userId_fkey")
  @@map("dbMedicamento")
}

model usuario {
  userId        Int             @id @default(autoincrement())
  email         String          @unique
  senha         String
  dbMedicamento dbMedicamento[]
  @@map("usuario")
}
